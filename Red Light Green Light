Broke.Name = "Broke"
Broke.Parent = main
Broke.BackgroundColor3 = Color3.fromRGB(65, 65, 65)
Broke.Position = UDim2.new(0.213235289, 0, 0.25454545, 0)
Broke.Size = UDim2.new(0, 90, 0, 17)
Broke.Font = Enum.Font.SourceSans
Broke.Text = "red light green light"
Broke.TextColor3 = Color3.fromRGB(113, 113, 113)
Broke.TextSize = 14.000
Broke.MouseButton1Down:connect(function()
local plr = game:service"Players".LocalPlayer;
local tween_s = game:service"TweenService";
local info = TweenInfo.new(16,Enum.EasingStyle.Quad);
function tp(...)
   local tic_k = tick(5);
   local params = {...};
   local cframe = CFrame.new(params[1],params[2],params[3]);
   local tween,err = pcall(function()
       local tween = tween_s:Create(plr.Character["HumanoidRootPart"],info,{CFrame=cframe});
       tween:Play();
   end)
   if not tween then return err end
end
tp(-313.14785766602, 2.9999992847443, 419.82235717773)
end)

TugOfWar.Name = "TogOfWar"
TugOfWar.Parent = main
TugOfWar.BackgroundColor3 = Color3.fromRGB(65, 65, 65)
TugOfWar.Position = UDim2.new(0.161764711, 0, 0.454545468, 0)
TugOfWar.Size = UDim2.new(0, 99, 0, 19)
