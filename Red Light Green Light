TugOfWar.Text = "Tog of war (off)"
TugOfWar.TextColor3 = Color3.fromRGB(0, 0, 0)
TugOfWar.TextSize = 14.000
TugOfWar.MouseButton1Down:connect(function()
	if TugOfWarsShut == true then 
		TugOfWarsShut = false 
	else
		TugOfWarsShut = true
		end
	if TugOfWarsShut == true then 
	    _G.cooll = true
	    TugOfWar.Text = "Tog of war (on)"
	    TugOfWar.BackgroundColor3 = Color3.fromRGB(0, 128, 0)
	end
	if TugOfWarsShut == false then 
	    TugOfWar.Text = "Tog of war (off)"
	    TugOfWar.BackgroundColor3 = Color3.fromRGB(65, 65, 65)
	end
	
	if TugOfWarsShut == true then
        while _G.cooll == true do  
game:GetService("ReplicatedStorage").Pull:FireServer(1)
game:GetService("ReplicatedStorage").Pull:FireServer(1)
game:GetService("ReplicatedStorage").Pull:FireServer(1)
game:GetService("ReplicatedStorage").Pull:FireServer(1)
game:GetService("ReplicatedStorage").Pull:FireServer(1)
game:GetService("ReplicatedStorage").Pull:FireServer(1)
game:GetService("ReplicatedStorage").Pull:FireServer(1)
game:GetService("ReplicatedStorage").Pull:FireServer(1)
game:GetService("ReplicatedStorage").Pull:FireServer(1)
game:GetService("ReplicatedStorage").Pull:FireServer(1)
game:GetService("ReplicatedStorage").Pull:FireServer(1)
game:GetService("ReplicatedStorage").Pull:FireServer(1)
wait()
        end
        end
	if TugOfWarsShut == false then 
		_G.cooll = false
	end
end)

